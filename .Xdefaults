!!urxvt config

!URxvt.perl-ext-common : default,keyboard-select,url-select,clipboard
!Urxvt.perl-ext-common: default, selection-to-clipboard

! URxvt.keysym.M-x           : perl:keyboard-select:activate
! URxvt.keysym.M-u           : perl:url-select:select_next
! URxvt.keysym.M-c           : perl:clipboard:copy
! URxvt.keysym.M-v           : perl:clipboard:paste
! URxvt.keysym.M-s           : perl:keyboard-select:search
! URxvt.keysym.M-C-v         : perl:clipboard:paste_escaped
! URxvt.url-select.launcher  : firefox
! URxvt.url-select.autocopy  : true
! URxvt.url-select.underline : true

!Disabled perl
! URxvt.perl-ext:
! URxvt.perl-ext-common:

URxvt.urlLauncher: firefox

! URxvt.font        : xft:Monaco:size=10
URxvt.font        : xft:Consolas:size=13
!URxvt.boldFont: xft:Consolas:style=Bold
!URxvt.boldItalicFont: xft:DejaVu Sans Mono:style=Bold Oblique
!URxvt.font	  : xft:Droid Sans Mono for Powerline:size=12
! Urxvt.font	  : xft:Inconsolata:size=10:antialias=false
! URxvt.font     : xft:Andale Mono:size=10:antialias=false
!URxvt.font   : xft:Monaco for Powerline:size=12
! URxvt.font:     xft:bitstream vera sans mono:size=10:antialias=true
! URxvt.boldFont: xft:bitstream vera sans mono:bold:size=10:antialias=true
!URxvt.font   : xft:DejaVu Sans Mono for Powerline:size=10


! Transparency
! URxvt*transparent:		true
! URxvt*shading:			10


Emacs*useXIM: false
Emacs*menubar*Foreground:        Gray30
Emacs*menubar*Background:        Gray75
!Emacs*.geometry:	80x77

URxvt.letterSpace : -1
URxvt.termName    : rxvt-256color
!URxvt.termName     : xterm-256color
! URxvt.iso14755    : false
URxvt.inputMethod  : ibus

URxvt.iconFile:    /usr/share/icons/gnome/32x32/apps/terminal.png

URxvt.urgentOnBell: true

URxvt.saveLines   : 15800
URxvt.scrollBar   : false
URxvt.scrollstyle : rxvt

!XFT
Xft.lcdfilter : lcddefault
Xft.hintstyle : hintslight
Xft.hinting   : true
Xft.antialias : true
Xft.rgba      : rgb

!Fading
URxvt.fading    : 30
URxvt.fadeColor : #333333

!Solarized + tangerine + random colours
!URxvt.cursorColor            : #93A1A1
URxvt*cursorBlink:            true
URxvt.cursorColor             : #4d4d4d
URxvt.pointerColorBackground : #586E75
URxvt.pointerColorForeground : #93A1A1

!URxvt.background : #101010
URxvt.background:  #000000
URxvt.foreground : #f2f2f2


